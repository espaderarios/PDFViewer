# Configuration file for deployment
# Copy this to .env and fill in your values

# Cloudflare Worker URL (after deployment)
WORKER_URL=https://your-worker.your-subdomain.workers.dev

# Frontend URL (Cloudflare Pages URL)
APP_URL=https://pdf-viewer.pages.dev

# Google OAuth Credentials
# Get these from https://console.cloud.google.com/
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Cloudflare IDs (from wrangler commands)
KV_NAMESPACE_ID=your-kv-namespace-id
D1_DATABASE_ID=your-d1-database-id
R2_BUCKET_NAME=pdf-storage
